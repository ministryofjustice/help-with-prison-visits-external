{
  "name": "help-with-prison-visits-external",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ministryofjustice/help-with-prison-visits-external.git"
  },
  "engines": {
    "node": "^16",
    "npm": "^8"
  },
  "scripts": {
    "record-build-info": "node scripts/record-build-info",
    "start": "npm run css-build && node ./app/bin/www",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "test": "standard && mocha --recursive test/unit/ --exit",
    "test-coverage": "nyc npm run test-unit",
    "test-unit": "mocha --recursive test/unit/ --exit",
    "test-integration": "KNEX_CONFIG=testing mocha --recursive test/integration/ --timeout 15000 --exit",
    "test-e2e": "KNEX_CONFIG=testing cypress run --config video=false",
    "test-e2e-ui": "KNEX_CONFIG=testing cypress open",
    "test-load": "cd test/load && ../../node_modules/.bin/artillery run scenarios.yaml cd ../../",
    "css-build": "./build-css"
  },
  "standard": {
    "ignore": [
      "jquery*.js",
      "ga.js"
    ],
    "globals": [
      "describe",
      "before",
      "beforeEach",
      "afterEach",
      "after",
      "it",
      "browser",
      "$",
      "cy",
      "expect"
    ]
  },
  "dependencies": {
    "applicationinsights": "^2.3.4",
    "aws-sdk": "^2.1191.0",
    "axios": "^0.27.2",
    "bases": "^0.2.1",
    "bluebird": "^3.7.2",
    "bunyan": "^1.8.15",
    "bunyan-prettystream": "^0.1.3",
    "clamscan": "^2.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "csurf": "^1.11.0",
    "debug": ">=4.3.4",
    "dotenv": "^16.0.1",
    "express": "~4.18.1",
    "govuk-frontend": "^4.3.0",
    "helmet": "^5.1.1",
    "i18n": "^0.15.0",
    "jquery": "^3.6.1",
    "jquery-ui-dist": "^1.13.2",
    "knex": "^2.2.0",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.4",
    "mssql": "^8.1.2",
    "multer": "^1.4.5-lts.1",
    "nunjucks": "^3.2.3",
    "on-finished": "^2.4.1",
    "path": "^0.12.7",
    "sanitize-html": "^2.7.0",
    "sass": "^1.54.4",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "cypress": "^10.10.0",
    "mocha": "^10.0.0",
    "node-mocks-http": "^1.11.0",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^14.0.0",
    "standard": "^17.0.0",
    "supertest": "^6.2.4"
  }
}
